<section class="list">
  <header>
    <h1>Hacemos buena pareja</h1>
    <%= render '/shared/user/sorting' %>
  </header>
  <% @interactions = true %>
  <% @timestamp = true %>
  
  <ul class="user-list clearfix">
    <%= render @users %>
  </ul>
  <div class="pagination">
    <%= pager_total(@users) %>
  </div>
  <%= paginate @users %>

</section>
