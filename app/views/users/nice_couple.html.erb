<section class="list">
  <header>
    <h1 class="pull-left"><%= _('Hacemos buena pareja') %></h1>
    <div class="sort-gustado">
      <%= render 'shared/user/sorting' %>
    </div>
    <div class="clearfix"></div>
  </header>
  <% @interactions = true %>
  <% @timestamp = true %>
  
  <ul class="user-list clearfix">
    <%= render @users %>
  </ul>
  <div class="pagination">
    <%= pager_total(@users) %>
  </div>
  <%= paginate @users %>

</section>
