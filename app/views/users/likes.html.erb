<section class="list">
  <header>
    <h1>Les he gustado</h1>
    <%= render '/shared/user/sorting' %>
  </header>
  <ul class="user-list clearfix">
    <% @timestamp = true %>
    <% @users.each do |user| %>
      <li><%= render '/shared/user/user-card', user: user %></li>
    <% end %>
  </ul>

  <%= render '/shared/user/pagination' %>

</section>
