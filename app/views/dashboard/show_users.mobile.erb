<section id="could-interest-me" class="home-users <%if params[:more]=='true' %>more-users<% end %>">
  	<div class="clearfix list-header">

    	<h4 class="pull-left"><%= _('Usuari@s que te pueden gustar') %></h4>

		<section id="related-list" class="vermas">
		  <div class="boxed">
		    <div class="boxed-inner">
		      <ul class="clearfix">
		        <% @best_suited_near_me.sample(48).each do |user| %>
		          <li class="<%= user.online? ? "online" : "" %> is-related">
		            <%= link_to user do %>
		              <%= profile_pic(user, width: 80, height: 80) %>
		            <% end %>
		          </li>
		        <% end %>
		      </ul>
		    </div>
		  </div>
		</section>
	</div>
</section>
